{
    "compilerOptions": {
      "module": "commonjs",
      "importHelpers":true,
      "noImplicitAny": true,
      "sourceMap": true,
      "esModuleInterop":true,
      "outDir": "dist",
      "baseUrl": ".",
      "noStrictGenericChecks":true,
      "moduleResolution": "node",
      "paths": {
        "@/*": ["src/*"],
        "*": ["node_modules/*"]
      },
    },
    "include": [
      "./src/"
    ]
  }
